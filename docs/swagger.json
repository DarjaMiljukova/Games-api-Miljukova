{
  "openapi": "3.0.2",
  "info": {
    "title": "Games-API",
    "version": "1.0.0",
    "description": "This is a Swagger UI for our Games API\n\nError handling\n\nErrors are presented using the HTTP status codes 400-599. The response include a more detailed explanation about the error in the body."
  },
  "paths": {
    "/games": {
      "get": {
        "tags": [
          "games"
        ],

        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "examples": {
                  "List of games[": {
                    "value": [
                      "Witcher 3",
                      "Cyberpunk 2077"
                    ]
                  }
                }
              }
            },
            "description": "Returns a list of games"
          }
        },
        "description": "Returns a list of all games in API"
      }
    }
  },
  "components": {}
}